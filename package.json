{
    "name": "gearsupport",
    "displayName": "Gear Support",
    "description": "Support and colorizer extension for Gear language",
    "version": "1.0.1",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gear",
            "aliases": ["Gear", "gear"],
            "extensions": [".gear"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "gear",
            "scopeName": "text.cs.gear",
            "path": "./syntaxes/gear.tmLanguage.json"
        }]
    }
}